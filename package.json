{
  "name": "puredit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "build-copy-files": "cp _prebuild_static/dsl.d.ts build/ && cp public/examples/db.d.ts build/examples/",
    "prebuild": "node ./scripts/build-types.mjs && npm run generate-dsl-declarations",
    "generate-dsl-declarations": "dts-bundle-generator -o public/prebuild/dsl.d.ts src/dsl/index.ts"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.19.12",
    "@codemirror/basic-setup": "^0.19.1",
    "@codemirror/commands": "^0.19.7",
    "@codemirror/lang-javascript": "^0.19.6",
    "@codemirror/lang-python": "^0.19.4",
    "@codemirror/language": "^0.19.7",
    "@codemirror/lint": "^0.19.3",
    "@codemirror/theme-one-dark": "^0.19.1",
    "@codemirror/view": "^0.19.40",
    "@typescript/vfs": "^1.3.5",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "string-similarity": "^4.0.4",
    "svelte": "^3.44.0",
    "svelte-dark-mode": "^2.1.0",
    "svelte-select": "^4.4.5",
    "web-tree-sitter": "0.20.3"
