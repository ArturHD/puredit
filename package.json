    "@types/lodash": "^4.14.178",
    "@types/string-similarity": "^4.0.0",
    "dts-bundle-generator": "^6.4.0",
    "fast-glob": "^3.2.11",
    "node-sass": "^7.0.1",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "zx": "^4.3.0"
  }
}
